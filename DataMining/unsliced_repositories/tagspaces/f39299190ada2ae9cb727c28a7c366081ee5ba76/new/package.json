{
  "name": "tagspaces",
  "version": "0.0.0",
  "description": "TagSpaces core repository",
  "homepage": "https://www.tagspaces.org",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "build-nc": "yarn version-meta && yarn build-main && yarn build-renderer && yarn build-worker",
    "build": "yarn version-meta && concurrently \"yarn build-main\" \"yarn build-renderer\" \"yarn build-worker\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors",
    "build-cordova": "cross-env NODE_ENV=production node --max-old-space-size=4096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.cordova.js --colors",
    "build-cordova-dev": "cross-env NODE_ENV=development node --max-old-space-size=4096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.cordova.js --colors",
    "build-web": "cross-env NODE_ENV=production node --max-old-space-size=4096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.web.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max-old-space-size=4096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "build-worker": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.worker.prod.js --colors",
    "build-worker-dev": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.worker.dev.js --colors",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-portable": "cross-env NODE_ENV=production electron ./app/ --portable",
    "prestart_bak": "yarn build",
    "flow": "flow check",
    "flow-typed": "rimraf flow-typed/npm && node --max-old-space-size=4096 node_modules/.bin/flow-typed install",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev.js",
    "postinstall": "concurrently \"yarn flow-typed\" \"yarn build\" \"yarn build-dll\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "cross-env START_HOT=1 yarn start-renderer-dev",
    "dev-build-worker": "yarn build-worker-dev && yarn dev",
    "bump-version": "yarn third-party && cd app && yarn version && cd .. && yarn version-meta",
    "update-i18n": "tx pull -a -d",
    "version-meta": "node scripts/versionmeta.js",
    "third-party": "yarn licenses generate-disclaimer --silent > app/third-party.txt",
    "package": "yarn build && yarn package-win && yarn package-linux && yarn package-mac && yarn package-web",
    "package-mac": "build --mac --x64 --config resources/builder.json",
    "package-mac-pro": "build --mac --x64 --config app/node_modules/@tagspaces/pro/resources/builder-pro.json",
    "package-win": "build --win --x64 --config resources/builder.json",
    "package-win-pro": "build --win --x64 --config app/node_modules/@tagspaces/pro/resources/builder-pro.json",
    "package-linux": "build --linux --config resources/builder.json",
    "package-linux-pro": "build --linux --config app/node_modules/@tagspaces/pro/resources/builder-pro.json",
    "package-pro": "node scripts/enablepro.js && yarn install-ext && yarn build && yarn package-web-pro && yarn package-clean && yarn package-win-pro && yarn package-linux-pro && yarn package-web-pro && yarn package-mac-pro",
    "package-web": "yarn prepare-web && yarn package-clean && zip -r ../builds/tagspaces-web.zip web -x web/files/**\\*",
    "package-android": "yarn build-cordova && cd cordova && yarn install && cordova build --release android && cd ../.. && ./prepare-android.sh",
    "package-web-pro": "yarn prepare-web && yarn package-clean && zip -r ../builds/tagspaces-pro-web.zip web -xi web/files/**\\* ",
    "package-clean": "rimraf app/dist/**/*.map && rimraf web/dist/**/*.map && rimraf app/node_modules/@tagspaces/pro/lib/ && rimraf app/node_modules/@tagspaces/pro/modules/ && rimraf app/node_modules/@tagspaces/pro/test/",
    "install-ext": "cd app && yarn install --force --prod --no-lockfile && cd ..",
    "setup-ext": "node scripts/setupextensions.js",
    "setup-pro": "node scripts/setuppro.js",
    "prepare-web": "rimraf web/dist && rimraf web/node_modules && cp -r app/node_modules web && cp LICENSE.txt web && yarn build-web",
    "prepare-cordova": "rimraf cordova/www/dist && rimraf cordova/www/node_modules && cp -Lr app/node_modules cordova/www && cp LICENSE.txt cordova/www",
    "run-web": "yarn prepare-web && node scripts/webdavserver.js",
    "run-android": "cd cordova && cordova run android && cd ..",
    "cordova-add-android": "cd cordova && cordova platform add android && cd ..",
    "cordova-create-app": "cordova create ./cordova org.tagspaces.mobile TagSpaces",
    "start-android": "yarn build-cordova && yarn run-android",
    "start-android-dev": "yarn prepare-cordova && yarn build-cordova-dev && yarn run-android",
    "update-changelog": "git log v3.1.5..HEAD --no-merges --format=%B > CHANGELOG.TXT",
    "clean": "rimraf app/dist && rimraf node_modules && rimraf app/node_modules",
    "source-map-app": "source-map-explorer app/dist/*.* --html reports/source-map-app.html",
    "source-map-web": "source-map-explorer web/dist/*.* --html reports/source-map-web.html",
    "source-map-cordova": "source-map-explorer cordova/www/dist/*.* --html reports/source-map-cordova.html",
    "source-map": "yarn source-map-app && yarn source-map-web && yarn source-map-cordova",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js",
    "test-all": "yarn lint && yarn flow && yarn test && yarn build && yarn test-e2e",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./tests/runTests.js e2e",
    "test-e2e-dev": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./tests/runTests.js e2e-dev"
  },
  "browserslist": "electron 2.0",
