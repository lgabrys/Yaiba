{
  "name": "TagSpaces Launcher",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A launcher for the TagSpaces extension",
  "icons": {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "64" : "icon64.png",
    "128": "icon128.png"  
  },
  "app": {
    "launch": {
	   "local_path": "index.html"
    }
  },
  "content_security_policy": "script-src 'self' ; object-src 'self'",  
  "homepage_url": "http://tagspaces.org"
}
